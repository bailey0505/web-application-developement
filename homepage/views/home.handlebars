<title>Bailey Rotellini</title>
    <script>
        AOS.init();
    </script>
    


<div id="homepage" data-aos="fade-in" data-aos-duration="1000">
<!--Table That Shows Current Classes -->
	<table class="table table-dark table-hover" style="margin-top: 15%; color: white;">
		<thead>
			<tr>
				<th><h3>Courses</h3></th>
			</tr>
		</thead>
		<tr>
			<th scope="col">Course Name</th>
			<th scope="col">Course Code</th>
			<th scope="col">Course Description</th>
			<th scope="col">Course Credits</th>
			<th scope="col">Course Professor</th>
		</tr>
		<tbody>
			{{#each classes}}
				<tr>
					<td>{{ class_name }}</td>
					<td>{{ class_code }}</td>
					<td>{{ class_description }}</td>
					<td>{{ credits }}</td>
					<td>{{ instructor }}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>
 